ot = document.getElementById('statusDot');\n    const statusText = document.getElementById('statusText');\n    const issueCount = document.getElementById('issueCount');\n    const issuesList = document.getElementById('issuesList');\n    \n    const icons = { pending: 'â³', fixed: 'ğŸŸ¡', 'pending-review': 'ğŸ”', resolved: 'âœ…', rejected: 'âŒ' };\n    \n    // WebSocket connection\n    function connectWS() {\n      try {\n        ws = new WebSocket(`ws://${location.hostname}:3001`);\n        ws.onopen = () => { statusDot.classList.add('connected'); statusText.textContent = 'Connected'; };\n        ws.onclose = () => { statusDot.classList.remove('connected'); statusText.textContent = 'Disconnected'; setTimeout(connectWS, 2000); };\n        ws.onerror = () => ws.close();\n        ws.onmessage = (e) => { if (e.data === 'reload') refresh(); };\n      } catch {}\n    }\n    \n    // Fetch issues\n    async function refresh() {\n      try {\n        const res = await fetch('/api/pending-issues?all=true');\n        const data = await res.json();\n        allIssues = (data.issues || []).sort((a, b) => {\n          const order = { pending: 0, fixed: 1, 'pending-review': 2, resolved: 3 };\n          const diff = (order[a.status] ?? 2) - (order[b.status] ?? 2);\n          return diff !== 0 ? diff : new Date(b.createdAt) - new Date(a.createdAt);\n        });\n        render();\n        updateCount();\n      } catch {\n        issuesList.innerHTML = '<div class=\"empty-state\">âš ï¸ Server not running<br><br><code>npm start</code></div>';\n      }\n    }\n    \n    function updateCount() {\n      const pending = allIssues.filter(i => i.status === 'pending').length;\n      const fixed = allIssues.filter(i => i.status === 'fixed').length;\n      let text = `${allIssues.length} issue${allIssues.length !== 1 ? 's' : ''}`;\n      if (pending) text += ` Â· ${pending} pending`;\n      if (fixed) text += ` Â· ${fixed} fixed`;\n      issueCount.textContent = text;\n    }\n\n    function initFilters() {\n      document.getElementById('statusSelect')?.addEventListener('change', (e) => { currentFilter = e.target.value; render(); });\n      document.getElementById('searchInput')?.addEventListener('input', (e) => { searchQuery = e.target.value; render(); });\n    }\n\n    // Run test\n    window.runTest = async function(issueId) {\n      runningTests.add(issueId);\n      render();\n      \n      try {\n        const res = await fetch('/api/run-issue-test', {\n          method: 'POST',\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify({ issueId })\n        });\n        const result = await res.json();\n        console.log('[Test Result]', JSON.stringify(result, null, 2)); // DEBUG\n        // Store in persistent Map so it survives refresh\n        testResults.set(issueId, result);\n        \n        const issue = allIssues.find(i => i.id === issueId);\n        if (issue && result.testPath) {\n          issue.testLink = result.testPath;\n        }\n        \n        const s = result.summary || {};\n        // Don't auto-resolve - let user see results first\n        // if (result.success && (s.passed || 0) > 0 && (s.failed || 0) === 0) {\n        //   await resolveWithTest(issueId, result);\n        // }\n      } catch (e) {\n        testResults.set(issueId, { success: false, error: e.message });\n      } finally {\n        runningTests.delete(issueId);\n        render();\n      }\n    };\n    \n    window.markFixed = async function(id) {\n      await fetch('/api/update-issue', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify({ issueId: id, status: 'fixed' }) });\n      refresh();\n    };\n    \n    async function resolveWithTest(issueId, testResult) {\n      const issue = allIssues.find(i => i.id === issueId);\n      const s = testResult.summary || {};\n      await fetch('/api/update-issue', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n        body: JSON.stringify({ issueId, status: 'resolved', resolution: `âœ… ${s.passed} passed`, testPassed: true, testPath: testResult.testPath })\n      });\n      setTimeout(refresh, 300);\n    }\n\n    // Render using wb-card elements\n    function render() {\n      const search = searchQuery.toLowerCase().trim();\n      const priorityMatch = search.match(/^priority:(\\d)$/);\n      const priorityFilter = priorityMatch ? priorityMatch[1] : null;\n      const textSearch = priorityMatch ? '' : search;\n      \n      const filtered = allIssues.filter(i => {\n        if (currentFilter !== 'all' && i.status !== currentFilter) return false;\n        if (priorityFilter && String(i.priority ?? '') !== priorityFilter) return false;\n        if (textSearch && !`${i.description} ${i.id} ${i.resolution}`.toLowerCase().includes(textSearch)) return false;\n        return true;\n      });\n      \n      if (!filtered.length) {\n        issuesList.innerHTML = '<div class=\"empty-state\">No issues found</div>';\n        return;\n      }\n      \n      issuesList.innerHTML = filtered.map(i => {\n        const date = new Date(i.createdAt).toLocaleString();\n        const desc = i.description || 'No description';\n        const firstLine = desc.split('\\n')[0];\n        const hasMore = desc !== firstLine;\n        const isRunning = runningTests.has(i.id);\n        const testResult = testResults.get(i.id);\n        if (testResult) console.log('[Render] testResult for', i.id, testResult); // DEBUG\n        const testLink = i.testLink || `tests/issues/issue-${i.id.replace(/[^a-zA-Z0-9]/g, '-').toLowerCase()}.spec.ts`;\n\n        let testResultHtml = '';\n        if (isRunning) {\n          testResultHtml = `<div class=\"issue-test__results issue-test__results--running\"><span class=\"spinner\"></span> Running...</div>`;\n        } else if (testResult) {\n          const s = testResult.summary || {};\n          const tests = testResult.tests || [];\n          \n          // Build steps HTML\n          let stepsHtml = '';\n          tests.forEach(t => {\n            if (t.steps && t.steps.length > 0) {\n              stepsHtml += t.steps.map(step => \n                `<div class=\"issue-test__step ${step.status === 'passed' ? 'issue-test__step--passed' : 'issue-test__step--failed'}\">\n                  ${step.status === 'passed' ? 'âœ“' : 'âœ—'} ${esc(step.title)}\n                </div>`\n              ).join('');\n            }\n          });\n          \n          testResultHtml = `<div class=\"issue-test__results issue-test__results--${testResult.success ? 'success' : 'failure'}\">\n            <div class=\"issue-test__summary\"><strong>${testResult.success ? 'âœ… PASSED' : 'âŒ FAILED'}</strong> - ${s.passed || 0} passed, ${s.failed || 0} failed</div>\n            ${stepsHtml ? `<div class=\"issue-test__steps\">${stepsHtml}</div>` : ''}\n          </div>`;\n        }\n\n        // Using simple div cards (no wb-card to avoid duplicate structure)\n        return `\n          <div class=\"issue-card\" data-status=\"${i.status}\">\n            <div class=\"issue-card__header\">\n              <h3 class=\"issue-card__title\">${esc(icons[i.status])} ${esc(firstLine)}</h3>\n              ${hasMore ? `<p class=\"issue-card__desc\">${esc(desc)}</p>` : ''}\n            </div>\n            <div class=\"issue-card__body\">\n              <div class=\"issue-meta\">\n                <span>ğŸ“… ${date}</span>\n                <span>Status: <strong>${i.status}</strong></span>\n                <span>ID: <code>${esc(i.id)}</code></span>\n                ${i.priority !== undefined ? `<span>Priority: ${i.priority}</span>` : ''}\n              </div>\n              ${i.resolution ? `<div class=\"issue-resolution\">ğŸ”§ ${esc(i.resolution)}</div>` : ''}\n              \n              <div class=\"issue-test\">\n                <div class=\"issue-test__title\">ğŸ§ª Validation Test</div>\n                <div class=\"issue-test__row\">\n                  <button class=\"btn btn--success btn--sm\" onclick=\"runTest('${i.id}')\" ${isRunning ? 'disabled' : ''}>\n                    ${isRunning ? '<span class=\"spinner\"></span>' : 'â–¶ï¸'} Run\n                  </button>\n                  <span class=\"issue-test__path\">${esc(testLink)}</span>\n                </div>\n                ${testResultHtml}\n              </div>\n            </div>\n            <div class=\"issue-card__footer\">\n              ${i.status === 'pending' ? `<button class=\"btn btn--warning btn--sm\" onclick=\"markFixed('${i.id}')\">ğŸŸ¡ Fixed</button>` : ''}\n              ${['pending','fixed'].includes(i.status) ? `<button class=\"btn btn--sm\" onclick=\"resolveIssue('${i.id}')\">âœ… Resolve</button>` : ''}\n              <button class=\"btn btn--sm\" onclick=\"viewTest('${i.id}')\">ğŸ“„ Test</button>\n              <button class=\"btn btn--sm\" onclick=\"toggleJson('${i.id}')\">ğŸ” JSON</button>\n              <button class=\"btn btn--danger btn--sm\" onclick=\"deleteIssue('${i.id}')\">ğŸ—‘ï¸</button>\n            </div>\n            <pre id=\"json-${i.id}\" class=\"issue-json\" style=\"display:none\">${esc(JSON.stringify(i, null, 2))}</pre>\n          </div>\n        `;\n      }).join('');\n\n    }\n\n    // View test\n    window.viewTest = async function(id) {\n      const issue = allIssues.find(i => i.id === id);\n      const testPath = issue?.testLink || `tests/issues/issue-${id.replace(/[^a-zA-Z0-9]/g, '-').toLowerCase()}.spec.ts`;\n      const desc = issue?.description || '';\n      const firstLine = desc.split('\\n')[0].substring(0, 60);\n      \n      try {\n        const res = await fetch(`/api/read-file?path=${encodeURIComponent(testPath)}`);\n        if (!res.ok) {\n          const template = `// ISSUE: ${firstLine}\\n// ID: ${id}\\n// ${desc.split('\\n').join('\\n// ')}\\n\\nimport { test, expect } from '@playwright/test';\\n\\ntest('verify fix', async ({ page }) => {\\n  await page.goto('http://localhost:3000/');\\n  // TODO: Add assertions\\n});`;\n          showModal('ğŸ§ª Template', testPath, template);\n          return;\n        }\n        const data = await res.json();\n        showModal('ğŸ§ª Test', testPath, data.content || '// Empty');\n      } catch (e) {\n        showModal('Error', testPath, `// ${e.message}`);\n      }\n    };\n    \n    function showModal(title, subtitle, content) {\n      document.getElementById('testModal')?.remove();\n      const modal = document.createElement('div');\n      modal.id = 'testModal';\n      modal.className = 'modal-overlay';\n      modal.innerHTML = `\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <div><div style=\"font-weight:600\">${title}</div><div style=\"font-family:monospace;font-size:0.75rem;color:var(--text-muted)\">${esc(subtitle)}</div></div>\n            <button onclick=\"document.getElementById('testModal').remove()\" style=\"background:none;border:none;font-size:1.5rem;cursor:pointer;color:var(--text-muted)\">âœ•</button>\n          </div>\n          <pre class=\"modal-body\">${esc(content)}</pre>\n        </div>\n      `;\n      modal.onclick = (e) => { if (e.target === modal) modal.remove(); };\n      document.body.appendChild(modal);\n      document.addEventListener('keydown', function close(e) { if (e.key === 'Escape') { modal.remove(); document.removeEventListener('keydown', close); } });\n    }\n\n    window.toggleJson = function(id) {\n      const el = document.getElementById('json-' + id);\n      if (el) el.style.display = el.style.display === 'none' ? 'block' : 'none';\n    };\n    \n    function esc(str) {\n      if (str == null) return '';\n      const div = document.createElement('div');\n      div.textContent = String(str);\n      return div.innerHTML;\n    }\n    \n    window.resolveIssue = async function(id) {\n      await fetch('/api/update-issue', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify({ issueId: id, status: 'resolved' }) });\n      refresh();\n    };\n    \n    window.deleteIssue = async function(id) {\n      if (!confirm('Delete?')) return;\n      await fetch('/api/delete-issue', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify({ issueId: id }) });\n      refresh();\n    };\n    \n    async function clearResolved() {\n      if (!confirm('Clear resolved?')) return;\n      await fetch('/api/clear-resolved-issues', { method: 'POST' });\n      refresh();\n    };\n    \n    // Expose to window for onclick handlers\n    window.refresh = refresh;\n    window.clearResolved = clearResolved;\n    \n    // Init\n    connectWS();\n    refresh();\n    initFilters();\n  </script>\n\n\n</body>    The fkn refesh button does not work.  fix it write a test and don't come back until it is fixed",
