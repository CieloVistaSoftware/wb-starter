{"target":"tests/issues/x-draggable-position.spec.ts","lines":["/**"," * Test: x-draggable Position Stability"," * Issue: note-1769212201021-p0"," */","import { test, expect } from '@playwright/test';","","test.describe('x-draggable Position Stability', () => {","  test.beforeEach(async ({ page }) => {","    await page.goto('http://localhost:3000/');","    await page.waitForLoadState('networkidle');","  });","","  test('x-draggable should not move element on init', async ({ page }) => {","    await test.step('Create test element', async () => {","      await page.evaluate(() => {","        const el = document.createElement('div');","        el.id = 'drag-test';","        el.style.cssText = 'position: absolute; left: 100px; top: 100px; width: 50px; height: 50px; background: blue;';","        el.textContent = 'Drag me';","        document.body.appendChild(el);","      });","    });","","    await test.step('Record initial position', async () => {","      const initialPos = await page.evaluate(() => {","        const el = document.getElementById('drag-test');","        const rect = el.getBoundingClientRect();","        return { left: rect.left, top: rect.top };","      });","      expect(initialPos.left).toBe(100);","      expect(initialPos.top).toBe(100);","    });",""]}