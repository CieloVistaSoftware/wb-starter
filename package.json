{
  "name": "wb-starter",
  "version": "3.0.0",
  "type": "module",
  "description": "Modern website starter kit powered by WB Behaviors",
  "main": "index.html",
  "scripts": {
    "start": "node scripts/update-docs-manifest.js && node server.js",
    "start:mcp": "npm --prefix mcp-server start",
    "mcp:healthcheck": "node scripts/mcp-healthcheck.js",
    "dev": "npx serve . -p 3000",
    "test:duplicates": "node scripts/test-duplicates.mjs",
    "test": "npm run test:fast",
    "test:fast": "node scripts/playwright-if-tests.js -- --max-failures=1",
    "test:dev": "node scripts/playwright-if-tests.js -- --project=base --project=behaviors",
    "test:inc": "node scripts/test-incremental.js",
    "test:ui": "npx playwright test --ui",
    "test:single": "node scripts/playwright-if-tests.js -- --workers=1",
    "pw:maybe": "node scripts/playwright-if-tests.js --",
    "test:compliance": "npx playwright test -p compliance",
    "test:regression": "npx playwright test -p regression",
    "test:base": "npx playwright test -p compliance -p base",
    "test:behaviors": "npx playwright test -p compliance -p base -p behaviors",
    "test:performance": "npx playwright test -p performance",
    "test:firefox": "npx playwright test -p firefox",
    "test:webkit": "npx playwright test -p webkit",
    "test:mobile": "npx playwright test -p mobile-chrome -p mobile-safari",
    "test:browsers": "npx playwright test -p firefox -p webkit -p mobile-chrome -p mobile-safari",
    "test:all": "npx playwright test -p compliance -p regression -p base -p behaviors -p performance",
    "test:integrity": "npx playwright test tests/compliance/project-integrity.spec.ts",
    "lint:md-powershell": "node scripts/lint-md-powershell.js",
    "lint:md-powershell:staged": "node scripts/lint-md-powershell-staged.js",
    "check:stale-locks": "node scripts/check-stale-locks.js",
    "check:staged-integrity": "node scripts/check-staged-integrity.js",
    "check:critical-scripts": "node scripts/ensure-critical-scripts.js",
    "cleanup:artifacts": "node scripts/cleanup-artifacts.js",
    "ci:docs-safety": "npm run lint:md-powershell && npm run check:stale-locks && npm run check:critical-scripts",
    "audit:schema-tests": "node scripts/audit-schema-tests.mjs",
    "generate:wb-tests": "node scripts/generate-wb-tests.mjs",
    "generate:schema-tests": "node scripts/generate-schema-tests.js",
    "generate:search": "node scripts/generate-search-index.js",
    "precommit": "npm run check:critical-scripts && npm run lint:md-powershell:staged && npm run check:stale-locks"
  },
  "keywords": [
    "website",
    "starter",
    "template",
    "wb-starter",
    "Components",
    "generator",
    "no-build",
    "config-driven"
  ],
  "author": "CieloVista Software",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CieloVistaSoftware/wb-starter.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "ws": "^8.18.3"
  },
  "customElements": "./data/custom-elements.json",
  "devDependencies": {
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "sharp": "^0.34.5"
  }
}
