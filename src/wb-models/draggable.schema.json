{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "draggable.schema.json",
  "title": "Draggable",
  "behavior": "draggable",
  "description": "Make an element draggable.",
  "compliance": {
    "baseClass": "wb-draggable",
    "supportLevel": "stable",
    "accessibility": "high"
  },
  "properties": {
    "axis": {
      "type": "string",
      "enum": [
        "x",
        "y",
        "both"
      ],
      "default": "both"
    },
    "handle": {
      "type": "string",
      "description": "Selector for drag handle"
    }
  },
  "$view": [],
  "$methods": {},
  "$cssAPI": {}
}