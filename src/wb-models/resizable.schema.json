{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "resizable.schema.json",
  "title": "Resizable",
  "description": "Make an element resizable.",
  "schemaFor": "resizable",
  "compliance": {
    "baseClass": "wb-resizable",
    "supportLevel": "stable",
    "accessibility": "high"
  },
  "properties": {
    "handles": {
      "type": "string",
      "default": "se"
    }
  },
  "$view": [],
  "$methods": {},
  "$cssAPI": {},
  "test": {
    "setup": [
      "<wb-resizable>Basic resizable content</wb-resizable>",
      "<wb-resizable handles=\"se\">handles=\"se\"</wb-resizable>",
      "<wb-resizable>Test permutation 3</wb-resizable>",
      "<wb-resizable>Test permutation 4</wb-resizable>",
      "<wb-resizable>Test permutation 5</wb-resizable>"
    ],
    "matrix": {
      "combinations": [
        {
          "handles": "se"
        }
      ]
    },
    "site": {
      "page": "interactive",
      "tag": "wb-resizable",
      "renders": true,
      "baseClass": "wb-resizable",
      "minInstances": 1,
      "checkText": [],
      "checkClasses": [],
      "checkAttributes": [
        "handles"
      ]
    }
  }
}
